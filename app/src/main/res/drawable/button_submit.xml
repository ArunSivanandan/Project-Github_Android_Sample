<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true"> <!-- Pressed, Active-->
        <shape >
            <solid android:color="#002835"/>
        </shape>
    </item>
    <item android:state_focused="false"> <!-- Not active-->
        <shape>
            <solid android:color="#004A61"/>
        </shape>
    </item>
</selector>